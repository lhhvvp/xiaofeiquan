.container {
  padding: 24rpx;
}

.notice {
  padding: 24rpx;
  color: #a00000;
  background: #fff5f5;
  border: 1px solid #ffd6d6;
  border-radius: 12rpx;
  margin-bottom: 16rpx;
}

.item {
  padding: 20rpx 0;
  border-bottom: 1px solid #eee;
}

.title {
  font-size: 32rpx;
  color: #222;
  line-height: 44rpx;
}

.sub {
  display: flex;
  justify-content: space-between;
  color: #888;
  font-size: 24rpx;
  margin-top: 10rpx;
}

