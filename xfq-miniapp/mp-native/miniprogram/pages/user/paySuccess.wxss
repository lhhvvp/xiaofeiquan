.container {
  padding: 200rpx 48rpx 48rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icon-wrap {
  width: 180rpx;
  height: 180rpx;
  border-radius: 999rpx;
  background: #22c55e;
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon {
  color: #fff;
  font-size: 80rpx;
  line-height: 1;
  font-weight: 800;
}

.title {
  margin-top: 24rpx;
  font-size: 38rpx;
  color: #303133;
}

.actions {
  width: 100%;
  margin-top: 120rpx;
  display: flex;
  flex-direction: column;
  gap: 24rpx;
}

