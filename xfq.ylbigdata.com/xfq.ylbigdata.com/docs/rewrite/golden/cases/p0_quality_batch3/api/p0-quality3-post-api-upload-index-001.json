{
  "id": "p0-quality3-post-api-upload-index-001",
  "meta": {
    "tier": "p0",
    "source": "manual",
    "app": "api",
    "controller": "Upload",
    "action": "index",
    "notes": "quality-batch3: multipart file upload deterministic error path"
  },
  "request": {
    "method": "POST",
    "path": "/api/upload/index",
    "query": {},
    "headers": {
      "Token": "${API_TOKEN}",
      "Userid": "${API_USERID}"
    },
    "body": {
      "type": "multipart",
      "parts": [
        {
          "name": "upload_type",
          "value": "file"
        },
        {
          "name": "file",
          "file_path": "upload-fixture.txt",
          "filename": "upload-fixture.txt",
          "content_type": "text/plain"
        }
      ]
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json; charset=utf-8"
    },
    "body": {
      "type": "json",
      "json": {
        "code": 0,
        "msg": "Failed to open temp directory.",
        "url": ""
      }
    }
  },
  "compare": {
    "mode": "json",
    "ignore_json_keys": [],
    "ignore_json_pointers": []
  }
}
